{
  "name": "@brunodb3/entity-physics",
  "description": "Simple entity physics for game worlds",
  "author": "Bruno Brito <brunodb3@gmail.com>",
  "private": false,
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc --declaration && tsc-alias -p tsconfig.json",
    "prestart": "yarn build",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
    "start:dev": "ts-node-dev --clear -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "three": "^0.139.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/three": "^0.139.0",
    "jest": "^27.4.0",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^3.14.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  }
}
